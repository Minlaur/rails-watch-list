<div class = "container">
<h1>Here are your watchlists! 🍿</h1>
  <ul>
    <% @lists.each do |list| %>
      <li><%= link_to list.name, list_path(list) %>
      <%= cl_image_tag list.photo.key, width: 400, height: 300, crop: :fill %>
      </li>
    <% end %>
  </ul>
<%= link_to "Add new list!", new_list_path %>
</div>
